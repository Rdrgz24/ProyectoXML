<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE test SYSTEM "estructura.dtd">
<test>
  <question id="1">
    <wording>¿Qué es la calistenia?</wording>
    <choices>
      <choice>Un tipo de yoga</choice>
      <choice correct="yes">Una disciplina de ejercicios de peso corporal</choice>
      <choice>Un deporte de equipo</choice>
      <choice>Un tipo de entrenamiento con pesas</choice>
    </choices>
  </question>
  <question id="2">
    <wording>¿Cuál de los siguientes es un ejercicio de calistenia?</wording>
    <choices>
      <choice>Peso muerto</choice>
      <choice>Press de banca</choice>
      <choice correct="yes">Fondos en paralelas</choice>
      <choice>Curl de bíceps</choice>
    </choices>
  </question>
  <question id="3">
    <wording>¿Qué grupo muscular trabaja principalmente la sentadilla?</wording>
    <choices>
      <choice>Trapezoides</choice>
      <choice>Abdomen</choice>
      <choice correct="yes">Piernas y glúteos</choice>
      <choice>Isquiotibiales</choice>
    </choices>
  </question>
  <question id="4">
    <wording>¿Qué es el street workout?</wording>
    <choices>
      <choice>Entrenamiento con pesas en gimnasio</choice>
      <choice>Yoga en la calle</choice>
      <choice>Entrenamiento aeróbico</choice>
      <choice correct="yes">Entrenamiento de calistenia en espacios públicos</choice>
    </choices>
  </question>
  <question id="5">
    <wording>¿En qué consiste el muscle-up?</wording>
    <choices>
      <choice>Es una sentadilla profunda con salto</choice>
      <choice>Es una flexión de brazos con aplauso</choice>
      <choice correct="yes">Es un ejercicio de calistenia que combina una dominada y un fondo</choice>
      <choice>Es una estocada con salto</choice>
    </choices>
  </question>
  <question id="6">
    <wording>¿Cuál es el beneficio principal de los fondos en paralelas?</wording>
    <choices>
      <choice>Trabajar la fuerza de las piernas</choice>
      <choice correct="yes">Fortalecer el pecho, hombros y tríceps</choice>
      <choice>Mejorar la flexibilidad</choice>
      <choice>Aumentar la resistencia cardiovascular</choice>
    </choices>
  </question>
  <question id="7">
    <wording>¿Qué significa la sigla "WOD" en el contexto de calistenia?</wording>
    <choices>
      <choice>Work of Dumbell</choice>
      <choice>Warm-up Over Degree</choice>
      <choice>Weights Over Death</choice>
      <choice correct="yes">Workout of the Day</choice>
    </choices>
  </question>
  <question id="8">
    <wording>¿Cómo se llama el ejercicio en el que te cuelgas de una barra y elevas las piernas rectas hasta formar un ángulo de 90 grados?</wording>
    <choices>
      <choice>Crunch de barra</choice>
      <choice correct="yes">Leg raises</choice>
      <choice>Flexión abdominal</choice>
      <choice>Sentadilla profunda</choice>
    </choices>
  </question>
  <question id="9">
    <wording>¿Qué equipo se usa principalmente en el street workout?</wording>
    <choices>
      <choice correct="yes">Barras horizontales y paralelas</choice>
      <choice>Pesas libres</choice>
      <choice>Máquinas de gimnasio</choice>
      <choice>Cuerda para saltar</choice>
    </choices>
  </question>
  <question id="10">
    <wording>¿Cuál es el objetivo principal de la calistenia?</wording>
    <choices>
      <choice>Perder peso rápidamente</choice>
      <choice correct="yes">Mejorar la fuerza y la resistencia utilizando el propio peso corporal</choice>
      <choice>Aumentar la masa muscular con pesas</choice>
      <choice>Mejorar la flexibilidad</choice>
    </choices>
  </question>
  <question id="11">
    <wording>¿Cuál es una variante avanzada de las sentadillas?</wording>
    <choices>
      <choice>Sentadillas normales</choice>
      <choice correct="yes">Sentadilla pistol (a una pierna)</choice>
      <choice>Sentadillas con salto</choice>
      <choice>Sentadillas isométricas</choice>
    </choices>
  </question>
  <question id="12">
    <wording>¿Qué tipo de ejercicio se hace en la calistenia?</wording>
    <choices>
      <choice>Ejercicio cardiovascular</choice>
      <choice>Ejercicio aeróbico</choice>
      <choice>Ejercicios de estiramiento</choice>
      <choice correct="yes">Ejercicio de fuerza</choice>
    </choices>
  </question>
  <question id="13">
    <wording>¿Cuál de los siguientes ejercicios es el más adecuado para trabajar el abdomen?</wording>
    <choices>
      <choice>Sentadillas</choice>
      <choice>Dominadas</choice>
      <choice>Plancha</choice>
      <choice correct="yes">Crunch abdominal</choice>
    </choices>
  </question>
  <question id="14">
    <wording>¿Qué es un "L-sit" en calistenia?</wording>
    <choices>
      <choice>Un ejercicio para el pectoral y la flexibilidad de piernas</choice>
      <choice correct="yes">Es un ejercicio en el que se mantiene el cuerpo suspendido en el aire con las piernas extendidas</choice>
      <choice>Un ejercicio para el pectoral</choice>
      <choice>Un ejercicio de sentadilla</choice>
    </choices>
  </question>
  <question id="15">
    <wording>¿Qué músculo principal se trabaja en las flexiones?</wording>
    <choices>
      <choice>Gemelos</choice>
      <choice correct="yes">Pectorales</choice>
      <choice>Cuádriceps</choice>
      <choice>Abdominales</choice>
    </choices>
  </question>
  <question id="16">
    <wording>¿Qué ejercicio se utiliza para mejorar el equilibrio en calistenia?</wording>
    <choices>
      <choice>Sentadillas</choice>
      <choice>Dominadas</choice>
      <choice correct="yes">Plancha lateral</choice>
      <choice>Curl de tríceps</choice>
    </choices>
  </question>
  <question id="17">
    <wording>¿Cuál es el propósito del "handstand push-up" o flexión en pino?</wording>
    <choices>
      <choice>Trabajar la parte inferior del cuerpo</choice>
      <choice correct="yes">Trabajar los hombros, tríceps y pectorales</choice>
      <choice>Mejorar la flexibilidad de las piernas</choice>
      <choice>Trabajar la espalda baja</choice>
    </choices>
  </question>
  <question id="18">
    <wording>¿Cuál de los siguientes no es un ejercicio común de calistenia?</wording>
    <choices>
      <choice correct="yes">Remo con barra</choice>
      <choice>Dominadas</choice>
      <choice>Plancha</choice>
      <choice>Sentadillas</choice>
    </choices>
  </question>
  <question id="19">
    <wording>¿Qué tipo de ejercicio es más efectivo para mejorar la movilidad articular?</wording>
    <choices>
      <choice>Ejercicios de fuerza</choice>
      <choice>Ejercicios cardiovasculares</choice>
      <choice correct="yes">Ejercicios de estiramiento</choice>
      <choice>Entrenamiento con pesas pesadas</choice>
    </choices>
  </question>
  <question id="20">
    <wording>¿Cuál es el ejercicio básico de calistenia para trabajar la zona lumbar?</wording>
    <choices>
      <choice>Sentadillas</choice>
      <choice>Dominadas</choice>
      <choice correct="yes">Superman</choice>
      <choice>Curl de bíceps</choice>
    </choices>
  </question>
</test>
