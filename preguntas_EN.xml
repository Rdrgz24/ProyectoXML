<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE test SYSTEM "estructura.dtd">
<test>
  <question id="1">
    <wording>What is calisthenics?</wording>
    <choices>
      <choice>A type of yoga</choice>
      <choice correct="yes">A discipline of bodyweight exercises</choice>
      <choice>A team sport</choice>
      <choice>A type of weight training</choice>
    </choices>
  </question>
  <question id="2">
    <wording>Which of the following is a calisthenics exercise?</wording>
    <choices>
      <choice>Deadlift</choice>
      <choice>Bench press</choice>
      <choice correct="yes">Dips on parallel bars</choice>
      <choice>Biceps curl</choice>
    </choices>
  </question>
  <question id="3">
    <wording>Which muscle group is mainly worked by squats?</wording>
    <choices>
      <choice>Trapezius</choice>
      <choice>Abs</choice>
      <choice correct="yes">Legs and glutes</choice>
      <choice>Hamstrings</choice>
    </choices>
  </question>
  <question id="4">
    <wording>What is street workout?</wording>
    <choices>
      <choice>Weight training in a gym</choice>
      <choice>Yoga on the street</choice>
      <choice>Aerobic training</choice>
      <choice correct="yes">Calisthenics training in public spaces</choice>
    </choices>
  </question>
  <question id="5">
    <wording>What is a muscle-up?</wording>
    <choices>
      <choice>A deep squat with a jump</choice>
      <choice>A push-up with a clap</choice>
      <choice correct="yes">A calisthenics exercise that combines a pull-up and a dip</choice>
      <choice>A jumping lunge</choice>
    </choices>
  </question>
  <question id="6">
    <wording>What is the main benefit of dips on parallel bars?</wording>
    <choices>
      <choice>Working leg strength</choice>
      <choice correct="yes">Strengthening the chest, shoulders, and triceps</choice>
      <choice>Improving flexibility</choice>
      <choice>Increasing cardiovascular endurance</choice>
    </choices>
  </question>
  <question id="7">
    <wording>What does the acronym "WOD" mean in the context of calisthenics?</wording>
    <choices>
      <choice>Work of Dumbell</choice>
      <choice>Warm-up Over Degree</choice>
      <choice>Weights Over Death</choice>
      <choice correct="yes">Workout of the Day</choice>
    </choices>
  </question>
  <question id="8">
    <wording>What is the exercise called where you hang from a bar and raise your legs straight to a 90-degree angle?</wording>
    <choices>
      <choice>Bar crunch</choice>
      <choice correct="yes">Leg raises</choice>
      <choice>Abdominal flexion</choice>
      <choice>Deep squat</choice>
    </choices>
  </question>
  <question id="9">
    <wording>What equipment is mainly used in street workout?</wording>
    <choices>
      <choice correct="yes">Horizontal and parallel bars</choice>
      <choice>Free weights</choice>
      <choice>Gym machines</choice>
      <choice>Jump rope</choice>
    </choices>
  </question>
  <question id="10">
    <wording>What is the main goal of calisthenics?</wording>
    <choices>
      <choice>Lose weight quickly</choice>
      <choice correct="yes">Improve strength and endurance using your own body weight</choice>
      <choice>Increase muscle mass with weights</choice>
      <choice>Improve flexibility</choice>
    </choices>
  </question>
  <question id="11">
    <wording>Which is an advanced squat variation?</wording>
    <choices>
      <choice>Regular squats</choice>
      <choice correct="yes">Pistol squat (one leg)</choice>
      <choice>Jump squats</choice>
      <choice>Isometric squats</choice>
    </choices>
  </question>
  <question id="12">
    <wording>What type of exercise is performed in calisthenics?</wording>
    <choices>
      <choice>Cardiovascular exercise</choice>
      <choice>Aerobic exercise</choice>
      <choice>Stretching exercises</choice>
      <choice correct="yes">Strength exercise</choice>
    </choices>
  </question>
  <question id="13">
    <wording>Which of the following exercises is best for working the abs?</wording>
    <choices>
      <choice>Squats</choice>
      <choice>Pull-ups</choice>
      <choice>Plank</choice>
      <choice correct="yes">Crunches</choice>
    </choices>
  </question>
  <question id="14">
    <wording>What is an "L-sit" in calisthenics?</wording>
    <choices>
      <choice>An exercise for the chest and leg flexibility</choice>
      <choice correct="yes">An exercise where you hold your body suspended in the air with legs extended</choice>
      <choice>An exercise for the chest</choice>
      <choice>A squat exercise</choice>
    </choices>
  </question>
  <question id="15">
    <wording>What is the main muscle worked in push-ups?</wording>
    <choices>
      <choice>Calves</choice>
      <choice correct="yes">Pectorals</choice>
      <choice>Quadriceps</choice>
      <choice>Abdominals</choice>
    </choices>
  </question>
  <question id="16">
    <wording>Which exercise is used to improve balance in calisthenics?</wording>
    <choices>
      <choice>Squats</choice>
      <choice>Pull-ups</choice>
      <choice correct="yes">Side plank</choice>
      <choice>Triceps curl</choice>
    </choices>
  </question>
  <question id="17">
    <wording>What is the purpose of the "handstand push-up"?</wording>
    <choices>
      <choice>Work the lower body</choice>
      <choice correct="yes">Work shoulders, triceps, and chest</choice>
      <choice>Improve leg flexibility</choice>
      <choice>Work lower back</choice>
    </choices>
  </question>
  <question id="18">
    <wording>Which of the following is not a common calisthenics exercise?</wording>
    <choices>
      <choice correct="yes">Barbell row</choice>
      <choice>Pull-ups</choice>
      <choice>Plank</choice>
      <choice>Squats</choice>
    </choices>
  </question>
  <question id="19">
    <wording>Which type of exercise is most effective for improving joint mobility?</wording>
    <choices>
      <choice>Strength exercises</choice>
      <choice>Cardiovascular exercises</choice>
      <choice correct="yes">Stretching exercises</choice>
      <choice>Heavy weight training</choice>
    </choices>
  </question>
  <question id="20">
    <wording>What is the basic calisthenics exercise to work the lower back?</wording>
    <choices>
      <choice>Squats</choice>
      <choice>Pull-ups</choice>
      <choice correct="yes">Superman</choice>
      <choice>Biceps curl</choice>
    </choices>
  </question>
</test>
